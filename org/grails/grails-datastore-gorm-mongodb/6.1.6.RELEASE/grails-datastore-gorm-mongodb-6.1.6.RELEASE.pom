      <groupId>org.grails</groupId>
      <artifactId>grails-datastore-gorm</artifactId>
      <version>6.1.10.RELEASE</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.grails</groupId>
      <artifactId>grails-datastore-gorm-bson</artifactId>
      <version>6.1.6.RELEASE</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.grails</groupId>
      <artifactId>grails-datastore-gorm-support</artifactId>
      <version>6.1.10.RELEASE</version>
